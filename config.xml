<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="2009720200" id="com.ask.jpak" version="2009720200" versionCode="2009720200" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ASK J-PAK FAST CASH TAX SERVICES</name>
    <description>ASK J-PAK FAST CASH TAX SERVICES</description>
    <author email="SBilancieri@metikmarketing.com" href="https://taxmobileapp.com/">Tax Mobile App</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <splash src="splash.png" />
    <access origin="*" />
    <allow-navigation href="http://ionic.local/*" />
    <allow-navigation href="*" />
    <allow-navigation href="maps:*" />
    <allow-navigation href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="maps:*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="1000" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="splash.png" />
    <preference name="SplashScreenDelay" value="6000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="icon.png" />
        <splash src="splash.png" />
        <preference name="SplashScreen" value="screen" />
        <preference name="AutoHideSplashScreen" value="true" />
        <splash density="land-hdpi" src="splash.png" />
        <splash density="land-ldpi" src="splash.png" />
        <splash density="land-mdpi" src="splash.png" />
        <splash density="land-xhdpi" src="splash.png" />
        <splash density="port-hdpi" src="splash.png" />
        <splash density="port-ldpi" src="splash.png" />
        <splash density="port-mdpi" src="splash.png" />
        <splash density="port-xhdpi" src="splash.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="icon.png" />
        <icon height="29" src="icon_29.png" width="29" />
        <icon height="40" src="icon_40.png" width="40" />
        <icon height="50" src="icon_50.png" width="50" />
        <icon height="57" src="icon_57.png" width="57" />
        <icon height="58" src="icon_58.png" width="58" />
        <icon height="72" src="icon_72.png" width="72" />
        <icon height="76" src="icon_76.png" width="76" />
        <icon height="80" src="icon_80.png" width="80" />
        <icon height="87" src="icon_87.png" width="87" />
        <icon height="100" src="icon_100.png" width="100" />
        <icon height="114" src="icon_114.png" width="114" />
        <icon height="120" src="icon_120.png" width="120" />
        <icon height="144" src="icon_144.png" width="144" />
        <icon height="152" src="icon_152.png" width="152" />
        <icon height="167" src="icon_167.png" width="167" />
        <splash height="480" platform="ios" src="Default.png" width="320" />
        <splash height="960" platform="ios" src="Default@2x.png" width="640" />
        <splash height="1136" platform="ios" src="Default-568h@2x.png" width="640" />
        <splash height="1334" platform="ios" src="Default-667h@2x.png" width="750" />
        <splash height="2208" platform="ios" src="Default-Portrait-736h@3x.png" width="1242" />
        <splash height="1242" platform="ios" src="Default-Landscape-736h@3x.png" width="2208" />
        <splash height="1024" platform="ios" src="Default-Portrait.png" width="768" />
        <splash height="768" platform="ios" src="Default-Landscape.png" width="1024" />
        <splash height="2048" platform="ios" src="Default-Portrait@2x.png" width="1536" />
        <splash height="1536" platform="ios" src="Default-Landscape@2x.png" width="2048" />
        <preference name="SplashScreen" value="res/screen/ios/Default@2x~universal~anyany.png" />
    </platform>
    <engine name="android" spec="~8.0.0" />
    <engine name="ios" spec="~5.0.1" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <plugin name="cordova-plugin-compat" spec="^1.1.0" />
    <plugin name="cordova-plugin-console" spec="^1.0.5" />
    <plugin name="cordova-plugin-device" spec="^1.1.4" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.1" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <engine name="browser" spec="~5.0.1" />
    <config-file mode="replace" parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
        <string>For auto-scanning your license and taking photos of your tax documents to send to the tax office.</string>
    </config-file>
    <plugin name="cordova-plugin-camera" spec="^2.4.1">
        <param name="CAMERA_USAGE_DESCRIPTION" value="For auto-scanning your license and taking photos of your tax documents to send to the tax office." />
        <param name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="For auto-scanning your license and taking photos of your tax documents to send to the tax office." />
    </plugin>
    <plugin name="phonegap-plugin-barcodescanner" spec="^7.0.0">
        <param name="CAMERA_USAGE_DESCRIPTION" value="For auto-scanning your license and taking photos of your tax documents to send to the tax office." />
        <param name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="For auto-scanning your license and taking photos of your tax documents to send to the tax office." />
    </plugin>
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.4.1" />
</widget>
